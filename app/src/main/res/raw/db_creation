CREATE TABLE RecentSearch (
    _id	INTEGER PRIMARY KEY AUTOINCREMENT,
    isText INT NOT NULL,
    textSearch TEXT,
    longitud DOUBLE,
    latitud DOUBLE,
    radio INT
);

CREATE TABLE User(
    _id INTEGER PRIMARY KEY AUTOINCREMENT,
    nombre TEXT,
    apellidos TEXT,
    profileImage TEXT,
    email TEXT UNIQUE,
    sexo CHAR,
    password TEXT,
    description TEXT
);